    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "html-webpack-plugin": "^5.6.5",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
